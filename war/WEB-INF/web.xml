<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>PorjetESIEE</servlet-name>
		<servlet-class>com.SI.PorjetESIEEServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PorjetESIEE</servlet-name>
		<url-pattern>/porjetesiee</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>createUser</servlet-name>
		<servlet-class>com.SI.createUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createUser</servlet-name>
		<url-pattern>/createUser</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>connectUser</servlet-name>
		<servlet-class>com.SI.connectUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>connectUser</servlet-name>
		<url-pattern>/connectUser</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddRequest</servlet-name>
		<servlet-class>com.SI.AddRequest</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddRequest</servlet-name>
		<url-pattern>/AddRequest</url-pattern>
	</servlet-mapping>
	
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
